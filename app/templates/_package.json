{
  "name": "<%= appname %>",
  "version": "<%= pkg.version %>",
  "description": "<%= pkg.description %>",
  "homepage": "<%= githubUrl %>/<%= filePrefix %>",
  "bugs": "<%= githubUrl %>/<%= filePrefix %>/issues",
  "author": {
    "name": "<%= realname %>",
    "url": "<%= githubUrl %>"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "<%= githubUrl %>/<%= filePrefix %>.git"
  },
  "scripts": {
    "start": "meshblu-connector-runner ./",
    "test": "istanbul cover _mocha -- -R spec --require coffee-coverage/register-istanbul",
    "package": "meshblu-connector-packager"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-coverage": "^1.0.1",
    "istanbul": "^0.4.2",
    "meshblu-connector-packager": "^11.0.3",
    "mocha": "^2.4.5",
    "mocha-istanbul": "^0.2.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "coffee-script": "^1.10.0",
    "debug": "^2.2.0",
    "eventemitter2": "^1.0.0",
    "meshblu-connector-runner": "^2.0.0"
  },
  "meshbluConnector": {
    "connectorAssemblerVersion": "v7.0.0",
    "dependencyManagerVersion": "v1.0.7",
    "ignitionVersion": "v1.0.6",
    "githubSlug": "<%= githubSlug %>",
    "schemasUrl": "https://raw.githubusercontent.com/<%= githubSlug %>/v1.0.0/schemas.json"
  },
  "platformDependencies": {
    "darwin": [],
    "win32": [],
    "win64": [],
    "linux": [],
    "android": [],
    "ios": []
  },
  "license": "MIT",
  "keywords": [
    "meshblu-connector",
    "gateblu"
  ]
}
